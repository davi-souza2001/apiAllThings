generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Feedback {
  id Int @id @default(autoincrement())

  type       String
  comment    String
  screenshot String?

  @@map("feedbacks")
}

model User {
  id Int @id @default(autoincrement())

  email String
  name String
  imageUser String?
  description String?

  @@map("users")
}
